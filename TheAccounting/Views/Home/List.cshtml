@using TheAccounting.Models.ViewModels
@model IEnumerable<FeeData>

@{
    Layout = null;
}

<div class="row">
    <div class="col-md-12">
        <table class="table table-bordered table-hover">
            <tr>
                <th>@Html.DisplayNameFor(model => model.Id)</th>
                <th>@Html.DisplayNameFor(model => model.FeeType)</th>
                <th>@Html.DisplayNameFor(model => model.FeeDay)</th>
                <th>@Html.DisplayNameFor(model => model.Amount)</th>
            </tr>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.Id)
                    </td>
                    <td>
                        @Enum.GetName(typeof(enFeeType),item.FeeType)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.FeeDay)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Amount)
                    </td>
                </tr>
            }
        </table>
    </div>
</div>